<?xml version='1.0' encoding='utf-8'?>
<widget id="com.interimproj.penguinssavechristmas" version="0.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>PenguinsSaveChristmas</name>
    <description>
        Addictive, penguin fueled, Christmas spirit pumping fun
    </description>
    <author email="interimproj@gmail.com" href="https://nean.io">
        InterimProj
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="Orientation" value="landscape" />
    <preference name="Fullscreen" value="true" />
    <preference name="DisallowOverscroll" value="true" />
    <engine name="android" spec="~7.1.4" />
    <engine name="ios" spec="~4.5.5" />
    <engine name="browser" spec="^5.0.4" />
</widget>
